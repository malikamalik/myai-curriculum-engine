{
  "name": "myai-curriculum-engine",
  "version": "1.0.0",
  "description": "Agentic curriculum engine for MyAIcademy",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "ingest": "node src/scripts/ingest.js",
    "watch": "node src/scripts/watch.js",
    "analyze": "node src/scripts/analyze.js",
    "api": "node src/api/server.js",
    "web": "node src/web/server.js",
    "dev": "concurrently \"npm run api\" \"npm run web\"",
